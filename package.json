{
  "name": "@fulfillmenttools/fulfillmenttools-sdk-typescript",
  "version": "0.1.6",
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "types": "dist/mjs/index.d.ts",
  "description": "fulfillmenttools TypeScript SDK",
  "author": "OC fulfillment GmbH (https://fulfillmenttools.com/)",
  "license": "MIT",
  "scripts": {
    "ci": "npm run build",
    "build": "rimraf ./dist && npm run build:cjs && npm run build:mjs",
    "build:cjs": "tsc --project tsconfig.cjs.json && cp res/package.cjs.json dist/cjs/package.json",
    "build:mjs": "tsc --project tsconfig.mjs.json && cp res/package.mjs.json dist/mjs/package.json",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prettier": "prettier --write 'src/**/*.{js,ts}'",
    "test": "jest --config jest.config.cjs",
    "test:watch": "jest --watch",
    "release": "release-it",
    "version": "auto-changelog -p && git add CHANGELOG.md"
  },
  "exports": {
    "import": "./dist/mjs/index.js",
    "require": "./dist/cjs/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fulfillmenttools/fulfillmenttools-sdk-typescript.git"
  },
  "bugs": {
    "url": "https://github.com/fulfillmenttools/fulfillmenttools-sdk-typescript/issues"
  },
  "homepage": "https://www.npmjs.com/package/@fulfillmenttools/fulfillmenttools-sdk-typescript",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "fulfillment",
    "fulfillmenttools",
    "nodejs",
    "typescript",
    "sdk",
    "sdk-integration"
  ],
  "devDependencies": {
    "@tsconfig/node16": "1.0.4",
    "@types/express": "4.17.20",
    "@types/jest": "29.5.6",
    "@types/jest-when": "3.5.4",
    "@types/node": "18.18.6",
    "@types/superagent": "4.1.20",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "auto-changelog": "2.4.0",
    "eslint": "8.46.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-eslint-plugin": "5.1.1",
    "eslint-plugin-jest": "27.2.3",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.6.4",
    "jest-extended": "3.2.4",
    "jest-when": "3.5.2",
    "prettier": "2.8.8",
    "release-it": "16.2.1",
    "rimraf": "5.0.5",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "date-fns": "2.30.0",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "http-status-enum": "1.0.2",
    "superagent": "8.1.2",
    "timers": "0.1.1",
    "tslog": "4.9.2"
  }
}
